<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
      <record model="ir.ui.view" id="s_warehouse_form_view">
        <field name="name">s.warehouse.form.view</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='code']" position="after">
                <field name="warehouse_lazada_code" string="Mã Kho Lazada" readonly="1" />
                <field name="is_warehouse_default" string="Là kho Lazada mặc định ?" readonly="1"/>
            </xpath>
        </field>
    </record>
</odoo>