            <xpath expr="//button[@name='action_open_label_layout']" position="after">
                <button type="object" name="btn_shipping_document" string="In nhÃ£n Lazada" attrs="{'invisible': [('package_lazada_id', '=', 'null')]}"/>
            </xpath>
             <xpath expr="//button[@name='action_open_label_layout']" position="after">
