<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="ecommerce_platform_form_view" model="ir.ui.view">
        <field name="name">ecommerce.platform.form.view</field>
        <field name="model">list.ecommerce</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record>

    <record id="ecommerce_platform_tree_view" model="ir.ui.view">
        <field name="name">ecommerce.platform.tree.view</field>
        <field name="model">list.ecommerce</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" string="Tên sàn TMĐT"/>
            </tree>
        </field>
    </record>

    <record id="ecommerce_platform_view" model="ir.actions.act_window">
        <field name="name">ecommerce.platform.view</field>
        <field name="res_model">list.ecommerce</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>